{
    "source": {
        "include": [
            "./app", 
            "./pages",
            "./utils",
            "./pages/api"
        ],
        "exclude": [
            "node_modules" 
        ],
        "includePattern": ".+\\.js(doc|x)?$", 
        "excludePattern": "(^|\\/|\\\\)_", 
        "recurse": true 
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "verbose": true
    }
}